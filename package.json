    "@babel/preset-env": "^7.23.2",
    "@babel/preset-typescript": "^7.23.2",
    "@types/argon2-browser": "^1.18.4",
    "@types/crypto-js": "^4.2.1",
    "@types/elliptic": "^6.4.17",
    "@types/event-stream": "^4.0.4",
    "@types/jest": "^29.5.7",
    "@types/node": "^20.8.10",
    "@types/node-forge": "^1.3.9",
